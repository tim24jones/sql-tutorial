--lesson 3 exercises
--1
select first, age from empinfo;
--2
select first, last, city from empinfo
    where city <> 'Payson';
--3
select * from empinfo
    where age > 40;
--4
select first,last from empinfo
    where last LIKE '%ay';
--5
select * from empinfo
    where first='Mary';
--6
select * from empinfo
    where first LIKE '%Mary%';
